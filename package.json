{
  "name": "@chimeraant/dev",
  "version": "0.0.0-semantic-release",
  "description": "Chimera Ant's development environment",
  "main": "lib/main.js",
  "scripts": {
    "build:ci": "tsc && ncc build --source-map --license licenses.txt",
    "build": "NODE_OPTIONS=--openssl-legacy-provider pnpm build:ci",
    "fix": "eslint --max-warnings=0 --ext .ts . --fix",
    "lint": "eslint --max-warnings=0 --ext .ts .",
    "pre-push": "pnpm pre-push:dirty && pnpm publish --dry-run",
    "pre-push:dirty": "CI=true pnpm install && pnpm build && pnpm lint",
    "node-version": "node --version"
  },
  "repository": "https://github.com/chimeraant/dev",
  "keywords": [
    "actions",
    "nix",
    "flake",
    "direnv"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "^3.0.6",
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.8",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.25.0",
    "js-yaml": "^4.1.0",
    "nazna": "^1.13.19",
    "pnpm": "^7.13.4",
    "semantic-release": "^19.0.5",
    "typescript": "^4.8.4"
  },
  "types": "./dist/types/index.d.ts",
  "module": "./dist/es6/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/es6/index.js"
  },
  "files": [
    "dist"
  ],
  "bin": "./dist/cli.js"
}
